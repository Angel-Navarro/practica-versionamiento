    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>

	  <!-- Version del proyecto -->
	  <AssemblyVersion>1.0.0.5</AssemblyVersion>
	  <FileVersion>1.0.0.5</FileVersion>
